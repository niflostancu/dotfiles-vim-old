{
    "python": {
        "venvPath": ".",
        "venv": ".venv"
    },
    "pyls": {
        "plugins":
        {
            "pyls_mypy":
            {
                "enabled": true,
                "live_mode": false
            }
        }
    },

    "texlab.rootDirectory": null,
    "texlab.auxDirectory": "out/",
    "texlab.build.args": [
        "-pvc"
    ],
    "texlab.build.onSave": false,
    "texlab.forwardSearch.executable": "okular",
    "texlab.forwardSearch.args": ["--unique", "file:%p#src:%l%f"]
}

